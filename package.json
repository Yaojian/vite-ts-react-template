{
  "name": "vite-ts-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@7.28.0",
  "scripts": {
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo .eslintcache",
    "lint": "eslint --cache src",
    "compile": "tsc --build",
    "test": "vitest run",
    "check": "run-s lint compile test",
    "watch": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
